<div class="instruct-header"><h2>Instructors</h2></div>
<div class="container">
<% @users.each do |user| %>
  <% if user.role == 'instructor' %>
  <div class="tile">
    <div class="instructor-image"><%= image_tag user.image %></div>
    <p><%= link_to user.name, user_path(user) %></p>
    </div>
  <% end %>
<% end %>
</div>