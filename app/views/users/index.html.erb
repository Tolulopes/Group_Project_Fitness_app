<%= search_form_for @q do |f| %>

  <%= f.label :name_cont %>
  <%= f.search_field :name_cont %>

  <%= f.submit %>
<% end %>
<h2>I am the index.html.erb file for instructors</h2>

<% @users.each do |user| %>
  <% if user.role == 'instructor' %>
    <p><%= link_to user.name, user_path(user) %></p>
  <% end %>
<% end %>